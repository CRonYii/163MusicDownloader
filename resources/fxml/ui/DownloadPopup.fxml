<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1"
      minWidth="200" minHeight="100" fx:controller="ca.bcit.cst.rongyi.ui.DownloadPopupController">
    <children>
        <JFXButton text="Playlist" onAction="#downloadPlaylist" prefWidth="Infinity"></JFXButton>
        <JFXButton text="Single Song" onAction="#downloadSong" prefWidth="Infinity"></JFXButton>
        <JFXButton text="Album" onAction="#downloadAlbum" prefWidth="Infinity"></JFXButton>
        <JFXButton text="All Songs of an Artist" onAction="#downloadArtist" prefWidth="Infinity"></JFXButton>
        <Separator orientation="HORIZONTAL"/>
        <JFXButton text="Open Songs Folder" onAction="#openSongsFolder" prefWidth="Infinity"></JFXButton>
    </children>
    <padding>
        <Insets top="5.0" bottom="5.0"/>
    </padding>
</VBox>
